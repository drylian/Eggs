{
  "name": "mta-accelerator",
  "main": "http/starter.js",
  "version": "1.1.1",
  "description": "Simples Http downloader(acelerador de download) para substituir o nginx no mta",
  "bin": "http/starter.js",
  "pkg": {
    "targets": [
      "latest-linux-x64"
    ],
    "scripts": [
      "http/**/*.js",
      "package.json"
    ],
    "assets": [
      "package.json"
    ],
    "outputPath": "build"
  },
  "scripts": {
    "dev": "node http/starter.js",
    "build": "pkg ."
  },
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^16.3.2",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "morgan": "^1.10.0",
    "pkg": "^5.8.1",
    "yargs": "^17.7.2",
    "zlib": "^1.0.5"
  },
  "license": "MIT"
}